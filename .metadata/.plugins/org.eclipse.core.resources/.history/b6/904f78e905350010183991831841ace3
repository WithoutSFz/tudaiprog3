package tphash;

import java.util.ArrayList;

public class Arbol <T extends Comparable<T>> {
	private NodoArbol raiz;
	private int cant;
	public Arbol() {
		this.raiz=null;
		this.cant=0;
	}
	public int getCant() {
		return cant;
	}
	public void setCant(int cant) {
		this.cant = cant;
	}
	
	public void add(T elem) {
		if(this.raiz==null)
			raiz=new NodoArbol<T>(elem);
		else 
			this.raiz.add(elem);
			
		}
}
